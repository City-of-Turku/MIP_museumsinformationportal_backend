	<queryString language="SQL">
		<![CDATA[SELECT kuva.nimi, kuva.id as kuva_id,
	kuva.otsikko,
	kuva.kuvaus,
	kuva.pvm_kuvaus,
	kuva.kuvaaja,
	kuva_alue.jarjestys
FROM kuva_alue
	INNER JOIN alue ON 
	 kuva_alue.alue_id = alue.id 
	INNER JOIN kuva ON 
	 kuva_alue.kuva_id = kuva.id
	 WHERE alue.id = $P{alue_id}
	 and kuva.poistettu is null
	 ORDER BY kuva_alue.jarjestys asc]]>
