	<subDataset name="Dataset-Aihe-Kysymykset" uuid="73d0b980-6644-4d19-ae92-2d7332b08be4">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="New Local"/>
		<parameter name="KysymysAiheID" class="java.lang.Long"/>
		<queryString language="SQL">
			<![CDATA[SELECT kys.kysymysteksti_fi AS kysymys_fi,
	kys.kysymysteksti_sv AS kysymys_sv,
	kys.kysymysteksti_en AS kysymys_en
FROM muistot_kysymys AS kys
WHERE 
	 kys.muistot_aihe_id = $P{KysymysAiheID}]]>
		</queryString>
		<field name="kysymys_fi" class="java.lang.String"/>
		<field name="kysymys_sv" class="java.lang.String"/>
		<field name="kysymys_en" class="java.lang.String"/>
	</subDataset>
	<parameter name="aihe_id" class="java.lang.Long">
		<parameterDescription><![CDATA[Aiheen ID]]></parameterDescription>
	</parameter>
	<parameter name="MIP_BACKEND_URL" class="java.lang.String">
		<parameterDescription><![CDATA[Backendin osoite]]></parameterDescription>
	</parameter>
	<parameter name="MIP_MAPSERVER_URL" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
        aihe.prikka_id as aihe_id,
        aihe.aihe_fi as otsikko_fi,
        aihe.aihe_sv as otsikko_sv,
        aihe.aihe_en as otsikko_en,
        aihe.esittely_fi as esittely_fi,
        aihe.esittely_sv as esittely_sv,
        aihe.esittely_en as esittely_en,
        aihe.aukeaa,
        aihe.sulkeutuu,
		st_xmin(bbox) as bbox_xmin, 
        st_ymin(bbox) as bbox_ymin, 
        st_xmax(bbox) as bbox_xmax, 
        st_ymax(bbox) as bbox_ymax,
        COALESCE(memory_count, 0) AS memory_count  -- To handle cases with no children, set count to 0
        FROM 
            muistot_aihe as aihe
           	LEFT JOIN (
        		SELECT 
        			muistot_aihe_id, 
        			ST_Extent(tapahtumapaikka) as bbox,
        			COUNT(*) AS memory_count
        		FROM muistot_muisto
        		GROUP BY muistot_aihe_id
    		) SQ_BBOX on (SQ_BBOX.muistot_aihe_id = aihe.prikka_id)	
        WHERE 
        		aihe.prikka_id = $P{aihe_id}
]]>
	</queryString>
	<field name="aihe_id" class="java.lang.Integer"/>
	<field name="otsikko_fi" class="java.lang.String"/>
	<field name="otsikko_sv" class="java.lang.String"/>
	<field name="otsikko_en" class="java.lang.String"/>
	<field name="esittely_fi" class="java.lang.String"/>
