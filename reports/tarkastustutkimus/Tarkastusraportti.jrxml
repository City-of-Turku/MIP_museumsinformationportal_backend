	<queryString language="SQL">
		<![CDATA[select 
--tutkimus
tu.id as tutkimus_id,
tu.nimi as tutkimus_nimi,
--tarkastus
ta.id as tarkastus_id, 
--kohde
k.id as kohde_id,
kkk.kunta,
kkk.kyla,
k.muinaisjaannostunnus,
k.nimi,
k.muutnimet,
kohdeajoitus.ajoitustarkenne,
kohdetyyppi.tyyppi,
kohdetyyppi.tarkenne,
--TODO: kohteen laajuus ja laajuuden arviointiperuste???
k.lukumaara,
kohdelaji.nimi_fi as kohdelaji,
k.etaisyystieto,
--TODO: sijaintikuvaus???
sijainti.sijainti_x, sijainti.sijainti_y,
k.korkeus_min, 
k.korkeus_max,
k.koordselite,
k.peruskarttanimi,
k.peruskarttanumero,
--kiinteistotunnus
kohde_kiinteisto.kiinteistotunnus,
--tarkastaja
tarkastaja.etunimi || ' ' || tarkastaja.sukunimi as tarkastaja,
tarkastaja.sahkoposti as tarkastaja_email,
tu.kenttatyo_alkupvm, 
tu.kenttatyo_loppupvm,
--tarkastuksen tiedot
ta.aiemmat_tiedot,
ta.aiemmat_loydot,
ta.tarkastusloydot,
ta.tarkastusolosuhteet,
ta.liitteet,
ta.muuta,
ta.tarkastuksen_syy,
ta.ymparisto_maasto,
ta.kohteen_kuvaus, --tarkempi kuvaus ja rajaus
ta.muut_tiedot,
ta.kohteen_kunto,
ta.hoitotarve,
ta.suoja_alueeksi,
ta.maankayttohankkeet,
ta.sailymisen_asiat,
ta.kohteen_tiedon_maara,
ark_kuva.kuva_id,
st_xmin(bbox) as bbox_xmin, 
st_ymin(bbox) as bbox_ymin, 
st_xmax(bbox) as bbox_xmax, 
st_ymax(bbox) as bbox_ymax,
tutkimusalueet.havainnot,
rajaustarkkuus.nimi_fi as rajaustarkkuus
from ark_tutkimus tu
left join ark_tarkastus ta on ta.ark_tutkimus_id = tu.id
left join kayttaja as tarkastaja on tarkastaja.id = ta.tarkastaja
left join ark_kohde_tutkimus ktu on ktu.ark_tutkimus_id = tu.id
left join ark_kohde k on k.id = ktu.ark_kohde_id
left join (
    select akk.ark_kohde_id, string_agg(ku.nimi, ', ') as kunta, string_agg(ky.nimi, ', ') as kyla, string_agg(ku.nimi_se, ', ') as kunta_se, string_agg(ky.nimi_se, ', ') as kyla_se
    from ark_kohde_kuntakyla akk
    left join kunta ku on akk.kunta_id = ku.id
    left join kyla ky on akk.kyla_id = ky.id
    group by akk.ark_kohde_id
) kkk on (kkk.ark_kohde_id = k.id)
left join (
	select kt.ark_kohde_id, string_agg(tyyppi.nimi_fi, ', ') as tyyppi, string_agg(tyyppi.nimi_se, ', ') as tyyppi_se, string_agg(tark.nimi_fi, ', ') as tarkenne, string_agg(tark.nimi_se, ', ') as tarkenne_se
	from ark_kohde_tyyppi kt
	left join ark_kohdetyyppi tyyppi on tyyppi.id = kt.tyyppi_id
	left join ark_kohdetyyppitarkenne tark on tark.id = kt.tyyppitarkenne_id
	group by kt.ark_kohde_id
) kohdetyyppi on (kohdetyyppi.ark_kohde_id = k.id)
left join (
	select ka.ark_kohde_id, string_agg(ajoitus.nimi_fi || ' - ' || tark.nimi_fi, ', ') as ajoitustarkenne, string_agg(ajoitus.nimi_se || ' - ' || tark.nimi_se, ', ') as ajoitustarkenne_se
	from ark_kohde_ajoitus ka
	left join ajoitus on ajoitus.id = ka.ajoitus_id
	left join ajoitustarkenne tark on tark.id = ka.ajoitustarkenne_id
	group by ka.ark_kohde_id
) kohdeajoitus on (kohdeajoitus.ark_kohde_id = k.id)
left join ark_kohdelaji kohdelaji on kohdelaji.id = k.ark_kohdelaji_id
left join (
	select kkr.ark_kohde_id, kkr.kiinteistotunnus
	from ark_kohde_kiinteistorakennus kkr
	where kkr.id in (
		select min(id) from ark_kohde_kiinteistorakennus group by ark_kohde_id
	)
) kohde_kiinteisto on kohde_kiinteisto.ark_kohde_id = k.id
left join (
	select ark_kuva.ark_tutkimus_id, ark_kuva.id as kuva_id
	from ark_kuva
	where ark_kuva.id in (
		select min(id) from ark_kuva group by ark_tutkimus_id
	) and ark_kuva.poistettu is null
) ark_kuva on ark_kuva.ark_tutkimus_id = tu.id
left join (
	select ks.kohde_id, ST_X(ST_CENTROID(ST_TRANSFORM(ks.sijainti, 3067))) as sijainti_x, ST_Y(ST_CENTROID(ST_TRANSFORM(ks.sijainti, 3067))) as sijainti_y 
	from ark_kohde_sijainti ks
	where ks.id in (
		select min(id) from ark_kohde_sijainti group by kohde_id
	)
) sijainti on (sijainti.kohde_id = k.id)
left join (
    select ks.kohde_id, ST_EXTENT(ks.sijainti) as bbox
	from ark_kohde_sijainti ks
	group by ks.kohde_id
) SQ_BBOX on (SQ_BBOX.kohde_id = k.id)
left join (
	select ark_tutkimusalue.ark_tutkimus_id, string_agg(ark_tutkimusalue.nimi || ': ' || ark_tutkimusalue.havainnot, '<br>') as havainnot
	from ark_tutkimusalue
	group by ark_tutkimusalue.ark_tutkimus_id
) tutkimusalueet on tutkimusalueet.ark_tutkimus_id = tu.id
left join rajaustarkkuus on rajaustarkkuus.id = k.rajaustarkkuus_id
where tu.id = $P{tutkimusId}]]>
